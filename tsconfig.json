{
  "compilerOptions":
  {
    "sourceMap": true,
    "target": "es2020",
    "module": "es2020",
    "lib": [
      "es2020", "DOM"
    ],
    "moduleResolution": "node",
    "verbatimModuleSyntax": true,
    "sourceMap": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./build"
  }
}
